../lib/$(PLAT)/libfaux.a: $(OBJS_FAUX) $(OBJS_CRF_MATH) $(OBJS_CRD_MATH) $(MKFS) faux/GNUmakefile
	$(AR) $(ARFLAGS) $@ $(OBJS_FAUX) $(OBJS_CRF_MATH) $(OBJS_CRD_MATH)

