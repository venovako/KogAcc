..\lib\$(PLAT)\ksvd2.lib: $(OBJS_KSVD2) $(MKFS) ksvd2\Makefile
	$(AR) $(ARFLAGS) /OUT:$@ $(OBJS_KSVD2)

..\obj\$(PLAT)\sksvd2.obj: ksvd2\sksvd2.F90 ksvd2\gksvd2.F90 $(MKFS) ksvd2\Makefile
	cd ..\obj\$(PLAT) && $(FC) $(FCFLAGS) /fpp /c ..\..\..\src\ksvd2\sksvd2.F90

